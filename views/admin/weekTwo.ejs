                var endWeek = new Date("3/14/2016");
                var tech = "techOne";

                showCustomer(divNum, name, address, email, temp, beginWeek, endWeek, tech);

              </script>
        </div>
      <% } %>
    <% } %>
    <div id="b1">
      <button class="btn btn-success" id="t1" onclick="emailTech('infoOne', '<%= techs[0].email %>', '<%= techs[0].name %>', 'Week of March 8th - March 14th    -Grass Cutters')">Send Email</button>
