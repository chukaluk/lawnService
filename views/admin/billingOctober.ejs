           <script>
                var divNum = "<%= i %>";

                var name = "<%= customers[i].name %>";
                var address = "<%= customers[i].address %>";
                var email = "<%= customers[i].email %>"; 
                var temp = "<%= customers[i].sDate %>";
                var tech = "techOne";
                var month = "October";
                var monthID = 9;

                monthlyBilling(divNum, name, address, email, temp, tech, month, monthID);

